{
  "name": "aws-console",
  "version": "4.0.0",
  "description": "AWS Console",
  "main": "./out/main/index.js",
  "author": "Philip Dowie <philip@jnawk.nz>",
  "homepage": "https://github.com/jnawk/aws-electron/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnawk/aws-electron.git"
  },
  "bugs": {
    "url": "https://github.com/jnawk/aws-electron/issues"
  },
  "keywords": [
    "AWS"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "models": "(cd models; npm run build)",
    "dev": "npm run models; electron-vite dev --clearScreen false",
    "build": "cd models && npm run build && cd .. && npm run typecheck && electron-vite build --clearScreen false",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@coreui/coreui": "^5.0.0",
    "@coreui/react": "^5.0.0",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "models": "file:models"
  },
  "devDependencies": {
    "@aws-sdk/client-sts": "^3.554.0",
    "@aws-sdk/credential-providers": "^3.554.0",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/ini": "^4.1.0",
    "@types/node": "^18.19.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/sprintf-js": "^1.1.4",
    "@vitejs/plugin-react": "^4.3.1",
    "debounce": "^2.0.0",
    "electron": "^31.3.1",
    "electron-builder": "^24.9.1",
    "electron-settings": "^4.0.4",
    "electron-updater": "^6.1.7",
    "electron-vite": "^2.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "ini": "^4.1.2",
    "prettier": "^3.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sprintf-js": "^1.1.3",
    "typescript": "^5.4.3",
    "urllib": "^3.23.0",
    "vite": "^5.4.0"
  }
}
